/* pages/shoppingcart/settle_account/settle_account.wxss */
@import '../index/index.wxss';
.settle_account_address_box{clear: both;width: 100%;min-height: 220rpx;background: url("data:image/png;base64,") #fff repeat-x left bottom;margin: 0 auto 20rpx;padding-bottom: 20rpx;padding-top: 20rpx;}
.settle_account_address_left{float: left;width: 100rpx;height: 200rpx;line-height: 200rpx;}
.settle_account_address_left_img{width: 60rpx;height: 60rpx;vertical-align:middle;margin-left: 20rpx;}
.settle_account_address_mid{float: left;width: 550rpx;height: 200rpx;}
.settle_account_address_contact{text-align: center;height:50rpx;color: #888;font-size: 32rpx;line-height: 50rpx;padding: 20rpx 10rpx 10rpx;}
.settle_account_address_info{padding: 10rpx;font-size: 24rpx;line-height: 36rpx;color: #888;}
.settle_account_address_right{float: left;width: 100rpx;height: 200rpx;line-height: 200rpx;}
.settle_account_address_right_img{width: 60rpx;height: 60rpx;vertical-align:middle;margin-left: 20rpx;}
.settle_account_address_tip{line-height: 200rpx;text-align: center;}
.settle_account_barbox{width: 100%;min-height: 100rpx;background: #fff;position: fixed;bottom: 0;}
.settle_account_barbox_left{float: left;width: 410rpx;min-height: 100rpx;line-height: 100rpx;text-align: left;padding: 0 20rpx;font-size: 30rpx;}
.settle_account_barbox_rightcur{float: left;width: 300rpx;min-height: 100rpx;color: #fff;line-height: 100rpx;text-align: center;background: #ff3b30;}
.settle_account_barbox_right{float: left;width: 300rpx;min-height: 100rpx;background: #ccc;color: #fff;line-height: 100rpx;text-align: center;}